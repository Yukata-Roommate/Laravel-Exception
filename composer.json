{
    "name": "yukata-roommate/laravel-exception",
    "description": "Easy to handle Exception for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": {
            "YukataRm\\Laravel\\Exception\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\Exception\\Provider\\FacadeServiceProvider",
                "YukataRm\\Laravel\\Exception\\Provider\\ViewServiceProvider",
                "YukataRm\\Laravel\\Exception\\Provider\\PublicationServiceProvider"
            ],
            "aliases": {
                "Handler": "YukataRm\\Laravel\\Exception\\Facade\\Handler"
            }
        }
    },
    "authors": [
        {
            "name": "Yukata Roommate",
            "email": "contact@yukata-roommate.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "^11.0",
        "yukata-roommate/laravel-provider": "^1.0.0",
        "yukata-roommate/laravel-logger": "^1.0.0",
        "yukata-roommate/laravel-mail": "^1.0.0"
    },
    "minimum-stability": "dev"
}